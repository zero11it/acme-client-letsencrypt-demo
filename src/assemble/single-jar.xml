<assembly>
	<id>single-jar</id>
	<formats>
		<format>jar</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<dependencySets>
		<dependencySet>
			<outputDirectory></outputDirectory>
			<useProjectArtifact>true</useProjectArtifact>
			<unpack>true</unpack>
		</dependencySet>
	</dependencySets>
	<fileSets>
		<fileSet>
			<directory>target/classes</directory>
			<outputDirectory></outputDirectory>
		</fileSet>
	</fileSets>
</assembly>